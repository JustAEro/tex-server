{
  "name": "tex-api",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start:prod": "node dist/main.js",
    "start:dev": "nodemon src/main.ts",
    "prepare": "echo $CI; if test '$CI' == ''; then husky install ; fi",
    "build": "tsc --project tsconfig.build.json",
    "build:docker": "docker build -t tex_server:v1 -f build/Dockerfile .",
    "run:docker": "docker run -d -p 4000:3000 tex_server:v1",
    "stop-remove:docker": "bash ./stop-remove-all.sh",
    "prettier": "prettier --write '{src}/**/*.{js,json,css,scss,less,md,ts,html,component.html}'",
    "prettier:check": "prettier --check '{src}/**/*.{js,json,css,scss,less,md,ts,html,component.html}'",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest --verbose --config test/jest.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "1.4.5-lts.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@types/express": "^4.17.15",
    "@types/express-serve-static-core": "^4.17.32",
    "@types/jest": "^29.2.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "axios": "^1.2.2",
    "commitlint": "^17.4.2",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^3.1.0",
    "prettier": "^2.8.2",
    "semantic-release": "^20.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
